<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>

  <servlet>
    <servlet-name>JsonServlet</servlet-name>
    <servlet-class>org.jason.web.servlet.JsonServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>JsonServlet</servlet-name>
    <url-pattern>/json</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>UserJsonServlet</servlet-name>
    <servlet-class>org.jason.web.servlet.UserJsonServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UserJsonServlet</servlet-name>
    <url-pattern>/user-json</url-pattern>
  </servlet-mapping>

  <servlet>
    <servlet-name>UsersJsonServlet</servlet-name>
    <servlet-class>org.jason.web.servlet.UsersJsonServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>UsersJsonServlet</servlet-name>
    <url-pattern>/users-json</url-pattern>
  </servlet-mapping>

</web-app>
